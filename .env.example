# Supabase Configuration
# Get these from your Supabase project settings -> API
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# YouTube Data API Configuration
# Get this from Google Cloud Console -> APIs & Services -> Credentials
# Make sure to enable YouTube Data API v3 in your project
YOUTUBE_API_KEY=your_youtube_api_key_here

# OpenAI API Configuration
# Get this from OpenAI Dashboard -> API Keys
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Configuration (Optional)
# Get this from Anthropic Dashboard -> API Keys
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Database Configuration
# These are used for vector database operations
# Get these from your Supabase project settings -> Database
DATABASE_URL=your_database_url_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Application Settings
# These can be left as default values
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

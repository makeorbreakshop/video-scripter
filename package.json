{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "(lsof -ti:3000 | xargs kill -9 2>/dev/null || true) && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "worker": "caffeinate -dims dotenv -e .env -- node worker.ts",
    "worker:title": "caffeinate -dims dotenv -e .env -- node title-vectorization-worker.ts",
    "worker:thumbnail": "caffeinate -dims dotenv -e .env -- node thumbnail-vectorization-worker.ts",
    "worker:format": "caffeinate -dims dotenv -e .env -- node format-classification-worker.ts",
    "worker:topic": "caffeinate -dims dotenv -e .env -- node topic-classification-worker.ts",
    "worker:classify": "caffeinate -dims dotenv -e .env -- node video-classification-worker.ts",
    "worker:pattern": "caffeinate -dims dotenv -e .env -- node workers/pattern-discovery-worker.js",
    "worker:view-tracking": "caffeinate -dims dotenv -e .env -- node workers/view-tracking-worker.js",
    "worker:view-tracking:init": "dotenv -e .env -- node workers/view-tracking-init.js",
    "worker:view-tracking:now": "dotenv -e .env -- node workers/view-tracking-run-now.js",
    "worker:llm-summary": "caffeinate -dims dotenv -e .env -- node workers/llm-summary-worker-speed-optimized.js",
    "worker:llm-summary-vectorization": "caffeinate -dims dotenv -e .env -- node workers/llm-summary-vectorization-worker.js",
    "worker:clustering": "dotenv -e .env -- node scripts/clustering/incremental/daily-clustering-worker.js",
    "workers": "concurrently -n \"IMPORT,TITLE,THUMB\" -c \"blue,green,magenta\" \"npm run worker\" \"npm run worker:title\" \"npm run worker:thumbnail\"",
    "workers:vectorization": "concurrently -n \"TITLE,THUMB\" -c \"green,magenta\" \"npm run worker:title\" \"npm run worker:thumbnail\"",
    "workers:classification": "concurrently -n \"FORMAT,TOPIC,VIDEO\" -c \"yellow,cyan,purple\" \"npm run worker:format\" \"npm run worker:topic\" \"npm run worker:classify\"",
    "workers:all": "concurrently -n \"IMPORT,TITLE,THUMB,FORMAT,TOPIC,VIDEO,PATTERN\" -c \"blue,green,magenta,yellow,cyan,purple,orange\" \"npm run worker\" \"npm run worker:title\" \"npm run worker:thumbnail\" \"npm run worker:format\" \"npm run worker:topic\" \"npm run worker:classify\" \"npm run worker:pattern\"",
    "test:search": "dotenv -e .env.local -- node scripts/test-multi-stage-search.js",
    "test:patterns": "dotenv -e .env -- node scripts/test-pattern-discovery.js",
    "test:patterns:full": "dotenv -e .env -- node --loader tsx tests/pattern-discovery-test-suite.js",
    "mcp": "cd mcp-server && npm run mcp",
    "mcp:setup": "cd mcp-server && npm run setup",
    "mcp:test": "cd mcp-server && npm run test",
    "mcp:health": "cd mcp-server && npm run health",
    "test:agentic": "dotenv -e .env -- tsx scripts/test-agentic-mode.ts",
    "test:agentic:mock": "tsx scripts/test-agentic-mock.ts",
    "test:agentic:full": "dotenv -e .env -- tsx scripts/test-agentic-comprehensive.ts",
    "test:patterns:unit": "dotenv -e .env -- node --loader tsx tests/unit-tests/pattern-analyzer-tests.js",
    "test:patterns:api": "dotenv -e .env -- node --loader tsx tests/integration-tests/api-integration-tests.js",
    "test:patterns:fix": "dotenv -e .env -- node --loader tsx test-and-fix-patterns.js",
    "test:patterns:all": "npm run test:patterns:unit && npm run test:patterns:api && npm run test:patterns:full",
    "test:patterns:quick": "dotenv -e .env -- node --loader tsx quick-pattern-test.js",
    "analyze-search": "node scripts/analyze-search-log.js",
    "test:pinecone": "dotenv -e .env -- tsx tests/quick-pinecone-test.ts",
    "test:system": "dotenv -e .env -- tsx tests/run-all-tests.ts",
    "test:health": "dotenv -e .env -- tsx tests/system-health-check.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
